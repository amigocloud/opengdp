baseurl="http://atlas.ca.gov/casil/imageryBaseMapsLandCover/imagery/naip/naip_2009/2009_NAIP_sid_county_compressions/"
#baseurl="file:///bigdata/naip/source/California.old/"

ramdisk="/mnt/ram2/"
tmp="/bigdata/tmp/"



##### setup proccess management #####

((limit=8))

source "@GENERICDIR@/generic.bash"
source "./dwh-proj_def.bash"


fetchpattern="*.sid"

datefunc="ca_naip_2009_dodate"

###############################################################################
# function to get a ts from a lftp command
###############################################################################

function ca_naip_2009_dodate {
    sed 's/.*/20090801/'
}


main "$@"
